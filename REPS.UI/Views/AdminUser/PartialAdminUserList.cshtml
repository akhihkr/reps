        </div> <!-- End of medium-6 small-12 -->
    </div> <!-- End of row reset-row -->


    <div id="user-details">
        @Html.Partial("~/Views/AdminUser/PartialUserDetails.cshtml")
    </div>


</div> <!-- End of box -->
